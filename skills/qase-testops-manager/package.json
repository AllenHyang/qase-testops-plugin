{
  "name": "qase-testops-manager",
  "version": "1.0.0",
  "description": "Qase TestOps Manager - Code First approach for test management",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "qase",
    "test-management",
    "playwright",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "csv-parse": "^6.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "scripts/**/*.js",
      "lib/**/*.js",
      "!scripts/full-sync.js"
    ]
  }
}
